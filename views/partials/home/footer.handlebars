    <script>
        $("[name='likes']").bootstrapSwitch();

        $(document).ready(() => {
            $("[name='likes']").on('switchChange.bootstrapSwitch', function(e, data) {
              const slug = $(this).attr('data-slug');
              $.ajax({
                  type: "PUT",
                  url: `/post/${slug}/like`, 
                  data: {},
                  cache: false,
                  success: function(data) {
                    toastr.success(`Post with slug ${data.slug} was updated.`);
                  }
              })
            })
        });
    </script>

